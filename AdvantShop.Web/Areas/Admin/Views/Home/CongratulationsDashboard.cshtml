@using AdvantShop.Track
@using AdvantShop.Trial

@*
    <a class="text-decoration-none" ng-click="congratulationsDashboard.skipDashboard()">@T("Admin.Home.Congratulations.SkipWizard")</a>
*@

@{
    Html.AddAsset("home");
    var isMobile = SettingsDesign.IsMobileTemplate;
}

@{
    var headerModel = new AdvantShop.Web.Admin.ViewModels.Shared.Common.HeaderViewModel();
    headerModel.Title = "<span class=\"float-header-page\">Настройка магазина</span>";
    headerModel.ShowOnlySticky = true;

    if (isMobile)
    {
        Layout = "~/Areas/Admin/Templates/Mobile/Views/Shared/_Layout.cshtml";
    }
}

@Html.Header(headerModel)

<div class="dashboard-content">
    <div class="dashboard-content__container">
        <div class="dashboard-content__list ng-cloak">
            @if (TrialService.IsTrialEnabled)
            {
                <div class="dashboard-content__item dashboard-content__item--black-bg dashboard-content__item--p-md dashboard-content__item--shadow @(isMobile ? "" : "dashboard-content__item--flex")" style="margin-bottom:16px;">
                    <div class="dashboard-content__item-about">
                        <p class="dashboard-content__text" style="@(isMobile ? "" : "color:#fff;")">@T("Admin.Home.CongratulationsDashboard.HaveTrial")</p>
                    </div>
                    <div class="dashboard-content__btn-blue-wrap">
                        <a class="btn btn-sm btn-success" href="@Url.AbsoluteActionUrl("Tariffs", "Service")">@T("Admin.Home.CongratulationsDashboard.SelectTariff")</a>
                    </div>
                </div>
            }
            <div class="dashboard-content__progress-bar-wrapper">
                <h3 class="dashboard-content__title dashboard-content__title--sub dashboard-content__title--m-b-sm" style="margin-top:16px;">@T("Admin.Home.CongratulationsDashboard.StoreProgress")</h3>
                <div class="dashboard-content__progress-bar dashboard-content__progress-bar--step-{{congratulationsDashboard.currentStepComplited}}"></div>
                <p class="dashboard-content__text dashboard-content__text--sub">@T("Admin.Home.CongratulationsDashboard.CompleteTasks")</p>
            </div>

            <div ng-if="!congratulationsDashboard.steps[0].Activated" class="dashboard-content__item dashboard-content__item--p-md dashboard-content__item--shadow">
                <h3 class="dashboard-content__title dashboard-content__title--m-b-sm">@T("Admin.Home.CongratulationsDashboard.QuickSetup")</h3>
                <p class="dashboard-content__text">@T("Admin.Home.CongratulationsDashboard.EnterCompanyInfo")</p>
                <form>
                    <div class="dashboard-content__form-block row">
                        <div class="dashboard-content__form-item col-xs-12">
                            <span class="dashboard-content__form-item--title">@T("Admin.Home.CongratulationsDashboard.StoreName") <span class="error-color">*</span></span>
                            <input class="form-control" type="text" ng-model="congratulationsDashboard.storeName" placeholder="@(isMobile ? T("Admin.Home.CongratulationsDashboard.StoreName").ToString() : "")" required validation-input-text="Название магазина" />
                        </div>

                        <div class="dashboard-content__form-item col-xs-12 col-md-4">
                            <span class="dashboard-content__form-item--title">@T("Admin.Home.CongratulationsDashboard.Country")</span>
                            <select ng-model="congratulationsDashboard.countryId"
                                    ng-change="congratulationsDashboard.getRegions()"
                                    ng-options="s.Value as s.Text for s in congratulationsDashboard.data.Countries"
                                    convert-to-number
                                    class="form-control">
                            </select>
                        </div>

                        <div class="dashboard-content__form-item col-xs-12 col-md-4" ng-show="congratulationsDashboard.hasRegions">
                            <span class="dashboard-content__form-item--title">@T("Admin.Home.CongratulationsDashboard.Region")</span>
                            <select ng-model="congratulationsDashboard.regionId"
                                    ng-options="s.Value as s.Text for s in congratulationsDashboard.regions"
                                    convert-to-number
                                    class="form-control">
                            </select>
                        </div>

                        <div class="dashboard-content__form-item col-xs-12 col-md-4">
                            <span class="dashboard-content__form-item--title">@T("Admin.Home.CongratulationsDashboard.City") <span class="error-color">*</span></span>
                            <input type="text" ng-model="congratulationsDashboard.city"
                                   autocompleter on-type="city"
                                   placeholder="Москва"
                                   required validation-input-text="@T("Admin.Home.CongratulationsDashboard.City")"
                                   class="form-control" />
                        </div>

                        <div class="dashboard-content__form-item col-xs-12 col-md-4">
                            <span class="dashboard-content__form-item--title">@T("Admin.Home.CongratulationsDashboard.PhoneNumber") <span class="error-color">*</span></span>
                            <input type="tel" ng-model="congratulationsDashboard.phone"
                                   placeholder="+7(800)455-20-40"
                                   autocomplete="off"
                                   mask_control
                                   mask-control-preset="phone"
                                   required
                                   validation-input-text="@T("Admin.Home.CongratulationsDashboard.PhoneNumber")"
                                   class="form-control" />
                            <span class="m-b-lg subtext-input @(isMobile ? "dashboard-content__text dashboard-content__text--sub" : "")">@T("Admin.Home.CongratulationsDashboard.DisplayClients")</span>
                        </div>
                    </div>
                    <div class="dashboard-content__btn-blue-wrap dashboard-content__btn-blue-wrap--right">
                        <button type="submit"
                                class="btn btn-sm btn-success"
                                button-validation button-validation-success="congratulationsDashboard.saveStoreInfo()">
                            @T("Admin.Home.CongratulationsDashboard.Next")
                        </button>
                    </div>
                </form>
            </div>

