@using AdvantShop.Trial

<div class="dashboard-content">
    <div class="dashboard-content__container">
        <div class="dashboard-content__list">
            @if (TrialService.IsTrialEnabled)
            {
            <div class="dashboard-content__item dashboard-content__item--black-bg dashboard-content__item--p-md dashboard-content__item--shadow dashboard-content__item--flex" style="margin-bottom:16px;">
                <div class="dashboard-content__item-about">
                    <p class="dashboard-content__text">@T("Admin.Home.SettingStoreDashboard.HaveTrial")</p>
                </div>
                <div class="dashboard-content__btn-blue-wrap">
                    <a class="dashboard-content__btn-blue">@T("Admin.Home.SettingStoreDashboard.SelectTariff")</a>
                </div>
            </div>
            }
            <div class="dashboard-content__progress-bar-wrapper">
                <h3 class="dashboard-content__title" style="margin-top:16px;">@T("Admin.Home.SettingStoreDashboard.StoreProgress")</h3>
                <div class="dashboard-content__progress-bar"></div>
                <p class="dashboard-content__text">@T("Admin.Home.SettingStoreDashboard.CompleteTasks")</p>
            </div>
            <div class="dashboard-content__item dashboard-content__item--p-md dashboard-content__item--shadow">
                <h3 class="dashboard-content__title">@T("Admin.Home.SettingStoreDashboard.QuickSetup")</h3>
                <p class="dashboard-content__text">@T("Admin.Home.SettingStoreDashboard.EnterCompanyInfo")</p>
                <div class="dashboard-content__form-block">
                    <div class="dashboard-content__form-item">
                        <label>
                            @T("Admin.Home.SettingStoreDashboard.StoreName")
                            <input style="max-width:500px;" placeholder="Shopogolik" />
                        </label>
                    </div>
                    <div class="dashboard-content__form-item">
                        <label>
                            @T("Admin.Home.SettingStoreDashboard.City")
                            <input style="max-width:300px;" placeholder="Москва" />
                        </label>
                    </div>
                    <div class="dashboard-content__form-item">
                        <label>
                            @T("Admin.Home.SettingStoreDashboard.PhoneNumber")
                            <input style="max-width:300px;" placeholder="8 800 455 20 40" />
                        </label>
                    </div>
                </div>
                <div class="dashboard-content__btn-blue-wrap dashboard-content__btn-blue-wrap--right">
                    <a class="dashboard-content__btn-blue">@T("Admin.Home.SettingStoreDashboard.Next")</a>
                </div>
            </div>
            <div class="dashboard-content__item dashboard-content__item--p-md dashboard-content__item--shadow dashboard-content__item--flex">
