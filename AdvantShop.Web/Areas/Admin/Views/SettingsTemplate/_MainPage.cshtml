@model AdvantShop.Web.Admin.Models.Settings.Templates.SettingsTemplateModel

@{
    var isMobile = SettingsDesign.IsMobileTemplate;
}

<div class="setting-title m-t-none">
    <h3 class="setting__subtitle">@T("Admin.Settings.Template.CarouselTitle")</h3>
</div>

<div class="form-group setting__form-group">
    <div class="row middle-xs control-label">
        <label for="settingsTemplateCarouselVisibility" class="col-xs col-md-3 col-wl-2 @(isMobile ? "flex-grow setting__font-weight--xs" : "")">
            @T("Admin.Settings.Template.CarouselVisibilityTitle")
        </label>
        <div class="col-xs-slim relative flex middle-xs first-xs last-md @(isMobile ? "p-r-xs" : "")">
            <label class="adv-checkbox-label flex">
                @Html.CheckBoxFor(x => x.CarouselVisibility, new { id = "settingsTemplateCarouselVisibility", @class = "adv-checkbox-input input-alt-mobile", ng_model = "settingsTemplate.CarouselVisibility" })
                <span class="adv-checkbox-emul" data-e2e="CarouselVisibility"></span>
            </label>
        </div>
        <help-trigger class="ng-cloak last-md  @(isMobile ? "m-l-sm p-l-sm m-r-sm" : "")" data-title="@T("Admin.Settings.Template.CarouselVisibilityTitle")">
            <div class="help-content">@T("Admin.Settings.Template.CarouselVisibilityHint")</div>
        </help-trigger>
    </div>
</div>

<div class="form-group row setting__form-group">
    <div class="col-xs-12 col-md-3 col-wl-2">
        <label for="settingsTemplateCarouselAnimationSpeed" class="control-label">
            @T("Admin.Settings.Template.CarouselAnimationSpeedTitle")
        </label>
    </div>
    <div class="col-xs-12 col-md-2 flex middle-xs relative">
        @Html.TextBoxFor(x => x.CarouselAnimationSpeed, new { id = "settingsTemplateCarouselAnimationSpeed", @class = "form-control input-small input-alt-mobile", ng_model = "settingsTemplate.CarouselAnimationSpeed" })
        <help-trigger class="help-trigger-icon-abs m-l-xs @(isMobile ? "p-l-sm" : "")" data-title=" @T("Admin.Settings.Template.CarouselAnimationSpeedTitle")">
            <div class="help-content">@T("Admin.Settings.Template.CarouselAnimationSpeedHint")</div>
        </help-trigger>
    </div>
</div>
<div class="form-group row setting__form-group">
    <div class="col-xs-12 col-md-3 col-wl-2">
