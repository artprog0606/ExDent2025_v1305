@model string
<!--noindex-->
<div data-oc-lazy-load="[{serie:true, files: [@Html.RenderAssetFilesList("cookiesPolicy", "")]}]">
    <div class="cookies-policy ng-cloak" data-cookies-policy-modal="true" data-ng-show="!cookiesPolicy.accepted" data-cookie-name="@Model">
        <div class="cookies-policy-block link-text-decoration-none cs-bg-3 cs-bg-1--mobile">
            <div class="cookies-policy-cell cs-t-3 cs-t-8--mobile">
                @Html.Raw(SettingsNotifications.CookiesPolicyMessageFormatted)
                <div class="cookies-policy-button">
                    <button data-ng-click="cookiesPolicy.accept()" type="button" class="btn btn-xsmall btn-action cookies-policy__btn-close">@T("Js.CookiesPolicy.Modal.Accept")</button>
                </div>
            </div>
        </div>
    </div>
</div>
<!--/noindex-->